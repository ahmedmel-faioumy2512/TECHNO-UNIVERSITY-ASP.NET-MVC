@model List<MVC_Training.Models.Student>

<h2>Students</h2>


<table cellpadding="0" cellspacing="0" width="90%">
    <thead>
        <tr>
            <th>Students ID</th>
            <th>Students Name</th>
            <th>Students Number</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td valign="middle" data-header-title="Id"><span>@student.studentId</span></td>
                <td valign="middle" data-header-title="Name"><a href="/Student/GetDetails/@student.studentId">@student.studentName</a></td>
                <td valign="middle" data-header-title="No">@student.studentNo</td>
                <td><a href="/Student/DeleteStudent/@student.studentId">Delete</a></td>
            </tr>

        }


    </tbody>
</table>



