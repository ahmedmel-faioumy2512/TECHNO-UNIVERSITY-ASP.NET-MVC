@model List<MVC_Training.Models.Teacher>

<h2>Teachers</h2>


<table cellpadding="0" cellspacing="0" width="90%">
    <thead>
        <tr>
            <th>Techer ID</th>
            <th>Teacher Name</th>
            <th>Teacher Number</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
       @foreach (var teacher in Model)
       {
        <tr>
            <td valign="middle" data-header-title="Id"><span>@teacher.teacherId</span></td>
            <td valign="middle" data-header-title="Name"><a href="/Teacher/GetDetails/@teacher.teacherId">@teacher.teacherName</a></td>
            <td valign="middle" data-header-title="No">@teacher.teacherNo</td>
            <td><a href="/Teacher/DeleteTeacher/@teacher.teacherId">Delete</a></td>
        </tr>

       }
                          

    </tbody>
</table>
    



